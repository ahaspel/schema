            "JobSubtype",
            "JobType",
            "Division",
            "LaborClass",
            "Language",
            "License",
            "Minority",
            "Skill",
            "Training",
            "Union",
            "State",
            "Attribute",
            "PurchaseOrderStatus",
            "Privilege",
            "BatchStatus",
            "TimeEntryType",
            "CompanyType",
            "JobStatus",
            "JobCertification",
            "WebPrivilege"
          ]
        },
        "Name": {
          "type": "string"
