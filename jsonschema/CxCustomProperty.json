{
	"id": "https://congistics.com/schema/CxCustomProperty",
	"title": "com.congistics.cbrd.util.CxCustomProperty",
	"definitions": {
		"CxDependency": {
			"type": "object",
			"properties": {
				"ObjectType": {
					"type": "string"
				},
				"Ids": {
					"type": "array",
					"items": {
						"type": "integer"
					}
				}
			},
			"required": [
				"ObjectType",
				"Ids"
			]
		},
		"IxDto": {
			"type": "object",
			"properties": {
				"ValidationStatus": {
					"type": "string",
					"enum": [
						"New",
						"Updated",
						"Deleted",
						"None",
						"SerializationFailure",
						"DatabaseFailure",
						"ValidationFailure"
					]
				},
				"ErrorMessage": {
					"type": "string"
				},
				"ErrorDetail": {
					"type": "string"
				},
				"IsValid": {
					"type": "boolean"
				}
			},
			"required": [
				"ValidationStatus",
				"ErrorMessage"
			]
		}
	},
	"immutable": [
		"DisplayName",
		"Dto"
	],
	"type": "object",
	"properties": {
		"SupportedTypes": {
			"type": "string",
			"enum": [
				"ActualTaskDay",
				"Allocation",
				"Employee",
				"Equipment",
				"Job",
				"JobActivity",
				"Material",
				"VirtualTaskDay"
			]
		},
		"AttributeType": {
			"type": "string",
			"enum": [
				"String",
				"Int",
				"Double",
				"Range",
				"Lookup"
			]
		},
		"ObjectType": {
			"type": "string",
			"enum": [
				"CustomProperty"
			]
		},
		"Id": {
			"type": "integer"
		},
		"Description": {
			"type": "string"
		},
		"ForeColor": {
			"type": "string"
		},
		"BackColor": {
			"type": "string"
		},
		"Name": {
			"type": "string",
			"minLength": 1
		},
		"Dependencies": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/CxDependency"
			}
		},
		"DisplayName": {
			"type": "string"
		}
	},
	"required": [
		"Id",
		"Name"
	]
}