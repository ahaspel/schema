					}
				}
			},
			"required": [
				"ObjectType",
				"Ids"
			]
		},
		"IxAddress": {
			"superRequired": [
				"Id",
				"Street",
				"City",
				"State"
			],
			"type": "object",
			"properties": {
				"Street": {
					"type": "string",
					"minLength": 1
				},
				"StreetDetails": {
					"type": "string"
				},
				"ZipCode": {
					"type": "string"
				},
				"City": {
					"type": "string",
					"minLength": 1
				},
				"State": {
					"$ref": "#/definitions/IxLookupValue"
				},
				"Geocode": {
					"$ref": "#/definitions/IxGeocode"
				},
				"Id": {
					"type": "integer"
				},
				"ObjectType": {
					"type": "string",
					"enum": [
						"Address"
					]
				}
			},
			"required": [
				"Street",
				"City",
				"State",
				"Id"
			]
		},
		"IxGeocode": {
			"type": "object",
			"properties": {
				"Lat": {
					"type": "number"
				},
				"Lng": {
					"type": "number"
				},
				"Source": {
					"type": "string",
					"enum": [
						"Action",
						"Owner",
						"GPSService"
					]
				}
			},
			"required": [
				"Lat",
				"Lng"
			]
		},
		"IxLookupValue": {
			"superRequired": [
				"Id",
				"Name",
				"ObjectType"
			],
			"type": "object",
			"properties": {
				"BaseObject": {
					"type": "string"
				},
				"Id": {
					"type": "integer"
				},
				"ObjectType": {
					"type": "string",
					"enum": [
						"JobPriority",
						"JobSubtype",
						"JobType",
						"Division",
						"LaborClass",
						"Language",
						"License",
						"Minority",
						"Skill",
						"Training",
						"Union",
						"State",
						"JobTaskStatus",
						"Attribute",
						"PurchaseOrderStatus",
						"TimeEntryType",
						"CompanyType",
						"JobStatus",
						"DocumentType",
						"TimeEntryLaborClass",
						"Privilege",
						"JobCertification"
					]
				},
				"Description": {
					"type": "string"
				},
				"Name": {
					"type": "string"
				},
				"Dependencies": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/CxDependency"
					}
				}
			},
			"required": [
				"Id"
			]
		}
	},
	"type": "object",
	"properties": {
		"Origin": {
			"$ref": "#/definitions/IxAddress"
		},
		"Destination": {
			"$ref": "#/definitions/IxAddress"
		},
		"Id": {
			"type": "integer"
		}
	},
	"required": [
		"Origin",
		"Destination",
		"Id"
	]
