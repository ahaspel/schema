{
	"id": "https://congistics.com/schema/CxTextMessage",
	"title": "com.congistics.cbrd.io.CxTextMessage",
	"definitions": {
		"CxDependency": {
			"type": "object",
			"properties": {
				"ObjectType": {
					"type": "string"
				},
				"Ids": {
					"type": "array",
					"items": {
						"type": "integer"
					}
				}
			},
			"required": [
				"ObjectType",
				"Ids"
			]
		},
		"IxAttribute": {
			"superRequired": [
				"AttributeId",
				"Id",
				"Value"
			],
			"type": "object",
			"properties": {
				"AttributeId": {
					"type": "integer"
				},
				"Value": {
					"type": "string"
				},
				"Id": {
					"type": "integer"
				},
				"Name": {
					"type": "string"
				},
				"Description": {
					"type": "string"
				}
			},
			"required": [
				"AttributeId",
				"Value",
				"Id"
			],
			"immutable": [
				"Name"
			]
		},
		"IxDto": {
			"type": "object",
			"properties": {
				"ValidationStatus": {
					"type": "string",
					"enum": [
						"New",
						"Updated",
						"Deleted",
						"None",
						"SerializationFailure",
						"DatabaseFailure",
						"ValidationFailure"
					]
				},
				"ErrorMessage": {
					"type": "string"
				},
				"ErrorDetail": {
					"type": "string"
				},
				"IsValid": {
					"type": "boolean"
				}
			},
			"required": [
				"ValidationStatus",
				"ErrorMessage"
			]
		},
		"IxLookupValue": {
			"type": "object",
			"properties": {
				"Dependencies": {
					"type": "array",
					"items": {
						"$ref": "#/definitions/CxDependency"
					}
				},
				"ObjectType": {
					"type": "string",
					"enum": [
						"JobPriority",
						"JobSubtype",
						"JobType",
						"Division",
						"LaborClass",
						"Language",
						"License",
						"Minority",
						"Skill",
						"Training",
						"Union",
						"State",
						"ToDoStatus",
						"Attribute",
						"ToDoPriority",
						"TimeEntryType",
						"CompanyType",
						"JobStatus",
						"DocumentType",
						"TimeEntryLaborClass",
						"JobCertification",
						"Privilege"
					]
				},
				"Id": {
					"type": "integer"
				},
				"Name": {
					"type": "string"
				},
				"Description": {
					"type": "string"
				}
			},
			"required": [
				"Id"
			]
		},
		"IxPhoneNumber": {
			"superRequired": [
				"Id",
				"PhoneNumber"
			],
			"type": "object",
			"properties": {
				"PhoneNumber": {
					"type": "string"
				},
				"NumericPhoneNumber": {
					"type": "string"
				},
				"Valid": {
					"type": "boolean"
				},
				"Id": {
					"type": "integer"
				},
				"Name": {
					"type": "string"
				},
				"Description": {
					"type": "string"
				}
			},
			"required": [
				"PhoneNumber",
				"Id"
			],
			"immutable": [
				"NumericPhoneNumber",
				"Valid"
			]
		}
	},
	"immutable": [
		"Received",
		"Sent"
	],
	"type": "object",
	"properties": {
		"ObjectType": {
			"type": "string",
			"enum": [
				"TextMessage"
			]
		},
		"Name": {
			"type": "string"
		},
		"Text": {
			"type": "string",
			"minLength": 1
		},
		"Sender": {
			"$ref": "#/definitions/IxPhoneNumber"
		},
		"Sent": {
			"type": "string",
			"format": "date-time"
		},
		"Recipient": {
			"$ref": "#/definitions/IxPhoneNumber"
		},
		"Received": {
			"type": "string",
			"format": "date-time"
		},
		"Id": {
			"type": "integer"
		},
		"Description": {
			"type": "string"
		},
		"Tags": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/IxAttribute"
			}
		},
		"Dependencies": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/CxDependency"
			}
		}
	},
	"required": [
		"Id",
		"Recipient",
		"Sender",
		"Text"
	]
}